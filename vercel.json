{
  "version": 2,
  "builds": [
    {
      "src": "Dockerfile",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "publish"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/api/app"
    }
  ],
  "env": {
    "ASPNETCORE_ENVIRONMENT": "Production"
  }
}